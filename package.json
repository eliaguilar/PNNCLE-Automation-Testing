{
  "name": "pnncle-automation-testing",
  "version": "1.0.0",
  "description": "Automated testing suite for pnncle.com using TypeScript and Playwright",
  "private": true,
  "scripts": {
    "test": "playwright test",
    "test:forms": "playwright test --grep @form_test",
    "test:content": "playwright test --grep @content_test",
    "test:ui": "playwright test --ui",
    "test:report": "playwright test --reporter=html",
    "test:headed": "playwright test --headed"
  },
  "keywords": [
    "playwright",
    "testing",
    "automation",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/cheerio": "^0.22.35",
    "@types/node": "^20.10.6",
    "cheerio": "^1.1.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "spellchecker": "^3.7.1"
  }
}
